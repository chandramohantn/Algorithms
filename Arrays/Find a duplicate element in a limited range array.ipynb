   "source": [
    "Given a limited range array of size n where array contains elements between 1 to n-1 with one element repeating, find the duplicate element in it"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "given_array = [1, 2, 3, 4, 4]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "# Time complexity: O(n)\n",
    "array_len = len(given_array)\n",
    "array_sum = sum(given_array)\n",
    "sum_n_numbers = int((array_len - 1) * (array_len) / 2)\n",
    "duplicate_element = array_sum - sum_n_numbers"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "4\n"
     ]
    }
   ],
   "source": [
    "print(duplicate_element)"
   ]
  },
  {
